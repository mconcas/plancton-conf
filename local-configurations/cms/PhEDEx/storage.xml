<storage-mapping>

<!-- Specific for Farm Usage  -->


<!-- LFN TO PFN -->

 <lfn-to-pfn protocol="rfio" destination-match=".*"
    path-match="/+store/(.*)" result="file:/storage/gpfs_tsm_cms/cms/disk/store/$1"/>

 <lfn-to-pfn protocol="srmv2" destination-match=".*"
    path-match="/+store/(.*)" result="srm://storm-fe-cms.cr.cnaf.infn.it:8444/srm/managerv2?SFN=/cmsdisk/store/$1"/>

 <lfn-to-pfn protocol="xrootd" destination-match=".*"
    path-match="/+store/(.*)" result="root://xrootd-cms-redirector.cr.cnaf.infn.it//store/$1"/>    


<!-- PFN TO LFN -->

  <pfn-to-lfn protocol="rfio" destination-match=".*"
    path-match=".*storage/gpfs_tsm_cms/cms/disk/(.*)" result="/$1"/>

  <pfn-to-lfn protocol="srmv2" destination-match=".*"
    path-match=".*\?SFN=/cmsdisk/(.*)" result="/$1"/>



</storage-mapping>
